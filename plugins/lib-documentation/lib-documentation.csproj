<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="../ServiceLib.targets" />

  <PropertyGroup>
    <!-- Extends base RootNamespace from ServiceLib.targets -->
    <RootNamespace>$(RootNamespace).Documentation</RootNamespace>
    <ServiceLib>documentation</ServiceLib>
  </PropertyGroup>

  <ItemGroup>
    <!-- Only packages NOT provided by bannou-service - NSwag, etc. come transitively -->
    <PackageReference Include="Markdig" Version="0.44.0" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc.Core" Version="2.3.0" />
    <PackageReference Include="System.ComponentModel.Annotations" Version="5.0.0" />
    <!-- Git repository binding dependencies -->
    <PackageReference Include="LibGit2Sharp" Version="0.30.0" />
    <PackageReference Include="YamlDotNet" Version="16.3.0" />
  </ItemGroup>

</Project>
