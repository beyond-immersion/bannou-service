STORYLINE_SCENARIO_TRIGGER_LOCK_TIMEOUT_SECONDS=30

# Timing/TTL Configuration
STORYLINE_PLAN_CACHE_TTL_SECONDS=3600
STORYLINE_SCENARIO_DEFINITION_CACHE_TTL_SECONDS=300
STORYLINE_SCENARIO_IDEMPOTENCY_TTL_SECONDS=3600

# =============================================================================
# Subscription Service Configuration (SUBSCRIPTION_*)
# =============================================================================
# Delay in seconds before background service starts processing
SUBSCRIPTION_STARTUP_DELAY_SECONDS=30

# Timing/TTL Configuration
SUBSCRIPTION_EXPIRATION_CHECK_INTERVAL_MINUTES=5
SUBSCRIPTION_EXPIRATION_GRACE_PERIOD_SECONDS=30

# =============================================================================
# Telemetry Service Configuration (TELEMETRY_*)
# =============================================================================
# OTLP exporter endpoint (gRPC or HTTP)
TELEMETRY_OTLP_ENDPOINT=http://localhost:4317
# OTLP transport protocol
TELEMETRY_OTLP_PROTOCOL=grpc
# Enable distributed tracing export
TELEMETRY_TRACING_ENABLED=true
TELEMETRY_TRACING_SAMPLING_RATIO=1.0
# Enable metrics export via Prometheus scraping endpoint (/metrics)
TELEMETRY_METRICS_ENABLED=true
# Service name for telemetry (defaults to effective app-id if not set)
TELEMETRY_SERVICE_NAME=
# Service namespace for telemetry grouping
TELEMETRY_SERVICE_NAMESPACE=bannou
# Deployment environment (development, staging, production)
TELEMETRY_DEPLOYMENT_ENVIRONMENT=development

# =============================================================================
# Voice Service Configuration (VOICE_*)
# =============================================================================
# Enable scaled tier voice communication (SIP-based)
VOICE_SCALED_TIER_ENABLED=false
# Enable automatic tier upgrade from P2P to scaled
VOICE_TIER_UPGRADE_ENABLED=false
# Migration deadline in milliseconds when upgrading tiers
VOICE_TIER_UPGRADE_MIGRATION_DEADLINE_MS=30000
# Maximum participants in P2P voice sessions
VOICE_P2P_MAX_PARTICIPANTS=8
# Maximum participants in scaled tier voice sessions
VOICE_SCALED_MAX_PARTICIPANTS=100
# Comma-separated list of STUN server URLs for WebRTC
VOICE_STUN_SERVERS=stun:stun.l.google.com:19302
VOICE_SIP_PASSWORD_SALT=your-sip-password-salt-for-voice
# SIP domain for voice communication
VOICE_SIP_DOMAIN=voice.bannou.local
# Kamailio SIP server host
VOICE_KAMAILIO_HOST=localhost
# Kamailio JSON-RPC port (typically 5080, not SIP port 5060)
VOICE_KAMAILIO_RPC_PORT=5080
# Kamailio SIP signaling port for client registration
VOICE_KAMAILIO_SIP_PORT=5060
# RTPEngine media relay host
VOICE_RTPENGINE_HOST=localhost
# RTPEngine control port
VOICE_RTPENGINE_PORT=22222
# Seconds to wait after startup before the first eviction cycle runs
VOICE_EVICTION_WORKER_INITIAL_DELAY_SECONDS=10
# How often the background worker checks for stale participants
VOICE_PARTICIPANT_EVICTION_CHECK_INTERVAL_SECONDS=15
# If true, joining a non-existent room auto-creates it with autoCleanup enabled
VOICE_AD_HOC_ROOMS_ENABLED=false
# Seconds an empty autoCleanup room persists before auto-deletion
VOICE_EMPTY_ROOM_GRACE_PERIOD_SECONDS=300

# Timing/TTL Configuration
VOICE_RTPENGINE_TIMEOUT_SECONDS=5
VOICE_KAMAILIO_REQUEST_TIMEOUT_SECONDS=5
VOICE_SIP_CREDENTIAL_EXPIRATION_HOURS=24
VOICE_PARTICIPANT_HEARTBEAT_TIMEOUT_SECONDS=60
VOICE_BROADCAST_CONSENT_TIMEOUT_SECONDS=30

# =============================================================================
# Worldstate Service Configuration (WORLDSTATE_*)
# =============================================================================
WORLDSTATE_DEFAULT_TIME_RATIO=24.0
WORLDSTATE_DEFAULT_DOWNTIME_POLICY=Advance
WORLDSTATE_MAX_CATCH_UP_GAME_DAYS=365
WORLDSTATE_BOUNDARY_EVENT_BATCH_SIZE=50
# Safety limit on calendar templates per game service.
WORLDSTATE_MAX_CALENDARS_PER_GAME_SERVICE=10
WORLDSTATE_RATIO_HISTORY_RETENTION_DAYS=90
WORLDSTATE_DEFAULT_CALENDAR_TEMPLATE_CODE=

# Distributed Lock Configuration
WORLDSTATE_CLOCK_TICK_INTERVAL_SECONDS=5
WORLDSTATE_CLOCK_TICK_STARTUP_DELAY_SECONDS=10
WORLDSTATE_CLOCK_CACHE_TTL_SECONDS=10
WORLDSTATE_DISTRIBUTED_LOCK_TIMEOUT_SECONDS=10

# Timing/TTL Configuration
WORLDSTATE_CALENDAR_CACHE_TTL_MINUTES=60

# =============================================================================
# Service Enable/Disable Flags (Loading Control)
# These control which services are loaded at startup
# =============================================================================
SERVICES_ENABLED=true
AUTH_SERVICE_ENABLED=true
ACCOUNT_SERVICE_ENABLED=true
PERMISSION_SERVICE_ENABLED=true
CONNECT_SERVICE_ENABLED=true
ORCHESTRATOR_SERVICE_ENABLED=true
BEHAVIOR_SERVICE_ENABLED=true
ACTOR_SERVICE_ENABLED=true
# Add {SERVICE}_SERVICE_DISABLED=true to explicitly disable a service

# =============================================================================
# Local Development Configuration
# =============================================================================
BANNOU_SERVICE_DOMAIN=localhost
HTTP_WEB_HOST_PORT=8080
HTTPS_WEB_HOST_PORT=8443
